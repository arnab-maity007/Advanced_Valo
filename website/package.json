{
  "name": "valorant-ai-commentary-web",
  "version": "1.0.0",
  "description": "Web interface for Valorant AI Commentary system - accessible platform for streamers",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "bridge": "python bridge/commentary_bridge.py",
    "dev-bridge": "python bridge/commentary_bridge.py --debug",
    "setup": "npm install && npm run setup-db",
    "setup-db": "node scripts/setup-database.js",
    "build": "webpack --mode production",
    "test": "jest",
    "lint": "eslint .",
    "format": "prettier --write ."
  },
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.7.4",
    "mongoose": "^8.0.3",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "multer": "^1.4.5-lts.1",
    "python-shell": "^5.0.0",
    "uuid": "^9.0.1",
    "moment": "^2.29.4",
    "node-cron": "^3.0.2",
    "ws": "^8.13.0",
    "axios": "^1.5.0",
    "form-data": "^4.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "keywords": [
    "valorant",
    "ai",
    "commentary",
    "streaming",
    "esports"
  ],
  "author": "Metamorph Team",
  "license": "MIT"
}
